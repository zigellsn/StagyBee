{
  "name": "stagybee",
  "version": "2.0.0-alpha01",
  "description": "StagyBee JS Dependencies",
  "private": true,
  "author": "Simon Zigelli",
  "scripts": {
    "build": "mix",
    "css": "tailwindcss -i ./style/all_main.css -o ./StagyBee/static/css/bundle.css",
    "css-watch": "tailwindcss -i ./style/main.css -o ./StagyBee/static/css/bundle.css --watch",
    "css-nginx": "tailwindcss -o ./../nginx/errorPage/502.css --content ./../nginx/errorPage/502.html --minify",
    "postcss": "postcss ./style/all_main.css -o ./StagyBee/static/css/bundle.css"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.1.1",
    "htmx-ext-ws": "^2.0.3",
    "htmx.org": "^2.0.6",
    "hyperscript.org": "^0.9.14",
    "laravel-mix": "^6.0.49",
    "luxon": "^3.7.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "purgecss": "^7.0.2",
    "sweetalert2": "^11.22.4",
    "tailwindcss": "^4.1.12",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/cli": "^4.1.12"
  }
}
