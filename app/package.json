{
  "name": "stagybee",
  "version": "2.0.0-alpha01",
  "description": "StagyBee JS Dependencies",
  "private": true,
  "author": "Simon Zigelli",
  "scripts": {
    "build": "mix",
    "css": "tailwindcss -i ./style/all_main.css -o ./StagyBee/static/css/bundle.css",
    "css-watch": "tailwindcss -i ./style/main.css -o ./StagyBee/static/css/bundle.css --watch",
    "css-nginx": "tailwindcss -o ./../nginx/502.css --content ./../nginx/502.html --minify",
    "postcss": "postcss ./style/all_main.css -o ./StagyBee/static/css/bundle.css"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.4.0",
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.7",
    "postcss-cli": "^9.1.0",
    "purgecss": "^4.1.3",
    "cssnano": "^5.1.0",
    "tailwindcss": "^3.0.23",
    "laravel-mix": "^6.0.43"
  },
  "dependencies": {
    "luxon": "^2.3.1",
    "sweetalert2": "^11.4.4",
    "htmx.org": "^1.7.0",
    "hyperscript.org": "^0.9.5"
  }
}
