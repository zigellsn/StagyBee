{
  "name": "stagybee",
  "version": "2.0.0-alpha01",
  "description": "StagyBee JS Dependencies",
  "private": true,
  "author": "Simon Zigelli",
  "scripts": {
    "build": "mix",
    "css": "tailwindcss -i ./style/all_main.css -o ./StagyBee/static/css/bundle.css",
    "css-watch": "tailwindcss -i ./style/main.css -o ./StagyBee/static/css/bundle.css --watch",
    "postcss": "postcss ./style/all_main.css -o ./StagyBee/static/css/bundle.css",
    "compress": "bread-compressor -a brotli -s ./StagyBee/static/css/*.css ./StagyBee/static/js/*.js"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.4.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "purgecss": "^4.1.3",
    "cssnano": "^5.0.14",
    "tailwindcss": "^3.0.7",
    "laravel-mix": "^6.0.39"
  },
  "dependencies": {
    "luxon": "^2.2.0",
    "sweetalert2": "^11.3.0",
    "htmx.org": "^1.6.1",
    "hyperscript.org": "^0.9.3"
  }
}
